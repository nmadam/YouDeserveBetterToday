<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.nancyadam.ydbt.entity.UserBook" table="user_book">
        <meta attribute="class-description">
            This class contains the user detail.
        </meta>
        <id name="userBookId" type="int" column="user_book_id">
            <generator class="native"/>
        </id>
        <property name="userId" column="user_id" type="int"/>
        <property name="rating" column="rating" type="string"/>
        <property name="comment" column="last_name" type="string"/>
    </class>
</hibernate-mapping>